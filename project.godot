config_version=5

[application]

config/name="Party Game"
run/main_scene="res://Core/SceneManager/root.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Scores="*res://Core/Scores/scores.gd"
Console="*res://Core/Console/console.gd"
Controls="*res://Core/Controls/controls.gd"
SceneManager="*res://Core/SceneManager/scene_manager.gd"
Screenshotter="*res://Core/Screenshotter/screenshotter.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/anthonyec.camera_preview/plugin.cfg")

